{
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/static-build" },
    { "src": "functions/api/contact.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/contact", "dest": "/functions/api/contact.js" },
    { "src": "/(.*)", "dest": "/frontend/build/$1" }
  ]
}
